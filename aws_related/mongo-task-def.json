{
    "family": "mongo",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::008757489742:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::008757489742:role/ecsTaskRole",
    "containerDefinitions": [
        {
            "name": "mongo",
            "image": "mongo:latest",
            "memory": 512,
            "cpu": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 27017,
                    "hostPort": 27017,
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/fitnessflowai-mongo",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
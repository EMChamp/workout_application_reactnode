{
    "family": "frontend",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::008757489742:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::008757489742:role/ecsTaskRole",
    "containerDefinitions": [
        {
            "name": "frontend",
            "image": "008757489742.dkr.ecr.us-east-1.amazonaws.com/fitnessflowai-frontend:latest",
            "memory": 512,
            "cpu": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 5041,
                    "hostPort": 5041,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "NEXT_PUBLIC_API_URL",
                    "value": "https://fitnessflowai.com"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/fitnessflowai-frontend",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}